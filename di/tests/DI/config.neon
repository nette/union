# Production site configuration data
production:
	webname: "the example"
	database:
		adapter: pdo_mysql
		params:
			host: db.example.com
			username: dbuser
			password: secret
			dbname: dbname

# Staging site configuration data inherits from production and
# overrides values as necessary
development < production:
	database:
		params:
			host: dev.example.com
			username: devuser
			password: devsecret
	timeout: 10
	display_errors: true
	html_errors: no
	items: {10, 20}

